import java.util.Arrays;

public class Colnotfixed {
    public static void main(String[] args) {
        int[][] arr={
                {1,2 ,3 ,4},
                {33,22,11,55,66},
                {12,6}
        };

        for (int[] a:arr) {
            System.out.println(Arrays.toString(a));
        }
    }
}
